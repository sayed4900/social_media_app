<%- include('./partials/auth-header.ejs') %>


<div class="container">
    <h1 class="title-feed">FEED</h1>
    <div class="feed">
        <ul class="image-list">
            <% posts.forEach(post => { %>
                <li class="">
                    <a href="/post/<%= post._id%>">
                        <div>
                            <img class="img" src="<%= post.image%>">
                        </div>
                    </a>
                </li>
                    
                <% }) %>                       
        </ul>
            
    </div>
</div>

<div class="col-6 mt-5">
    <a class="btn btn-primary" href="/">Return to Profile</a>
    
  </div>
<%- include('./partials/footer.ejs') %> 
